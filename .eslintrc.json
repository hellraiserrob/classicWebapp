{
    "env": {
        "browser": true
        // "node": true,
        // "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "eqeqeq": [
            "error",
            "always"
        ],
        "no-console": [
            0
        ],
        "no-unused-vars": [
            0
        ]
    },
    // "parserOptions": {
    //     "ecmaFeatures": {
    //         "jsx": true,
    //         "modules": true
    //     }
    // },
    "parserOptions": {
        "sourceType": "module"
    }
}